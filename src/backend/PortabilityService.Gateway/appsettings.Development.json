{
  "Gateway": {
    "GlobalConfiguration": {
      "BaseUrl": "http://localhost:2748"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} {Level:u3} | {RequestId} - {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Debug"
      }
    ]
  }
}